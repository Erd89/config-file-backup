# GitHub
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github
    IdentitiesOnly yes

# GitLab
Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/gitlab
    IdentitiesOnly yes

# Vps IONOS connection
Host myvps
  HostName 87.106.40.87
  User rosso
  IdentityFile ~/.ssh/id_ed25519_macbook_vps

# VM Kind
Host kind
  HostName 172.17.84.245
  User teambari
  IdentityFile ~/.ssh/kind
  IdentitiesOnly yes
  SetEnv TERM=xterm-256color
